{
  "name": "typescript-back-template",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "test": "NODE_ENV=test  && npx jest -i",
    "build": "NODE_ENV=production rm -rf ./dist && npx tsc -P ./tsconfig.json --outDir dist && babel dist -d dist",
    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node ./dist/server.js",
    "deploy": "npm run migration:run && git push heroku main",
    "dev": "NODE_ENV=development npx nodemon --watch \"src/**\" --ext \"ts,json\" --exec \" ts-node -r tsconfig-paths/register --files src/server.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate -n",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:create": "npm run build && npm run typeorm -- migration:create -n ",
    "eslint": "npx eslint src",
    "eslint:fix": "npx eslint --fix src",
    "seed": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node -r tsconfig-paths/register ./src/seeders",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/email-validator": "1.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/joi": "17.2.3",
    "@types/js-yaml": "4.0.2",
    "@types/jsonwebtoken": "8.5.4",
    "@types/node": "16.3.3",
    "@types/pg": "8.6.1",
    "@types/readline-sync": "1.4.4",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "git-commit-msg-linter": "3.2.8",
    "husky": "7.0.2",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.12",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "cors": "2.8.5",
    "dayjs": "1.10.6",
    "dotenv": "10.0.0",
    "email-validator": "2.0.4",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "http-status": "1.5.0",
    "joi": "17.4.2",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "pg": "8.6.0",
    "readline-sync": "1.4.10",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.34",
    "typescript-transform-paths": "3.3.1",
    "uuid": "8.3.2"
  }
}
